{
  "name": "@apicurio/api-lifecycle-hub-ui",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "license": "Apache-2.0",
  "repository": "https://github.com/Apicurio/apicurio-api-lifecycle-hub.git",
  "scripts": {
    "clean": "rimraf dist generated-src .kiota",
    "generate-hub-client": "cross-env KIOTA_VERSION=v1.8.1 kiota generate -l typescript -d ../hub/app/src/main/resources/META-INF/openapi.json -c LifecycleHubClient -o ./generated-src/hub-client",
    "generate-workflows-client": "cross-env KIOTA_VERSION=v1.8.1 kiota generate -l typescript -d ../workflows/app/src/main/resources/META-INF/openapi.json -c LifecycleWorkflowsClient -o ./generated-src/workflows-client",
    "kiota-info": "cross-env KIOTA_VERSION=v1.8.1 kiota info -l typescript",
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint --ext ts,tsx src"
  },
  "devDependencies": {
    "@apicurio/eslint-config": "0.2.0",
    "@kiota-community/kiota-gen": "1.0.1",
    "@types/luxon": "3.4.2",
    "@types/node": "20.11.17",
    "@types/react": "18.2.55",
    "@types/react-dom": "18.2.19",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "@vitejs/plugin-react-swc": "3.6.0",
    "cross-env": "7.0.3",
    "eslint": "8.56.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.5",
    "rimraf": "5.0.5",
    "shelljs": "0.8.5",
    "typescript": "5.3.3",
    "vite": "5.1.1",
    "vite-tsconfig-paths": "4.3.1"
  },
  "dependencies": {
    "@microsoft/kiota-abstractions": "1.0.0-preview.42",
    "@microsoft/kiota-http-fetchlibrary": "1.0.0-preview.41",
    "@microsoft/kiota-serialization-form": "1.0.0-preview.31",
    "@microsoft/kiota-serialization-json": "1.0.0-preview.42",
    "@microsoft/kiota-authentication-azure": "1.0.0-preview.37",
    "@microsoft/kiota-serialization-text": "1.0.0-preview.39",
    "@microsoft/kiota-serialization-multipart": "1.0.0-preview.21",

    "@patternfly/patternfly": "5.2.0",
    "@patternfly/react-core": "5.2.0",
    "@patternfly/react-icons": "5.2.0",
    "@patternfly/react-table": "5.2.0",
    "@patternfly/react-code-editor": "5.2.0",

    "@apicurio/common-ui-components": "2.0.0",
    "luxon": "3.4.4",
    "oidc-client-ts": "3.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.22.0",
    "use-resize-observer": "9.1.0"
  }
}
